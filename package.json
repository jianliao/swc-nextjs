{
  "name": "swc-nextjs-monorepo",
  "version": "0.0.4",
  "description": "",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "pnpm --parallel -r run build",
    "clean": "pnpm --parallel -r run clean",
    "prettier": "prettier --write .",
    "generate": "pnpm upgrade && node src/index && node src/icon-ui && node src/icon-workflow && prettier --write . && pnpm i && pnpm -r --parallel run build",
    "publish": "pnpm -r --filter @swc-nextjs/* publish --access public --report-summary",
    "pack": "pnpm --parallel --filter @swc-nextjs/* -r exec rm -fr node_modules && pnpm --parallel --filter @swc-nextjs/* -r exec npm pack"
  },
  "keywords": [
    "Next.js",
    "Spectrum Web Components"
  ],
  "workspaces": [
    "./components/**"
  ],
  "author": "jian.liao@gmail.com",
  "license": "MIT",
  "engines": {
    "node": ">=18.7.0",
    "pnpm": ">=7"
  },
